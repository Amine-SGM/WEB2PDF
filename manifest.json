{
  "manifest_version": 3,
  "name": "Web to PDF Converter",
  "version": "1.0",
  "description": "Convert web content to PDF format",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "printing"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      
      "60": "icons/icon60.png"
    }
  },
  "icons": {
  
    "60": "icons/icon60.png"
  
  }
}
